#include <iostream>


int main(){
	char salir;
	int n1,n2,n3,n4;
	std::cout<<"Dime el primer numero: ";
	std::cin>>n1;
	std::cout<<"Dime el segundo numero: ";
	std::cin>>n2;
	std::cout<<"Dime el tercero numero: ";
	std::cin>>n3;
	std::cout<<"Dime el cuarto numero: ";
	std::cin>>n4;
	std::cout<<n1*n2*n3*n4;
	std::cin>>salir;
}
